pipeline {
    agent any

    stages {
        stage ('Compile Stage') {

            steps {
                withMaven(maven : 'maven_3_6_3') {
                    sh 'mvn clean'
                }
            }
        }

        stage ('Validation  Stage') {

            steps {
                withMaven(maven : 'maven_3_6_3') {
                    sh 'mvn validate'
                }
            }
        }


        stage ('Build Stage') {
            steps {
                withMaven(maven : 'maven_3_6_3') {
                    sh 'mvn build'
                }
            }
        }
        stage ('Test Stage') {
            steps {
                withMaven(maven : 'maven_3_6_4') {
                    sh 'mvn test'
                }
            }
        }
        stage ('package Stage') {
            steps {
                withMaven(maven : 'maven_3_6_3') {
                    sh 'mvn package'
                }
            }
        }
         stage ('install Stage') {
            steps {
                withMaven(maven : 'maven_3_6_3') {
                    sh 'mvn install'
                }
            }
        }
    }
}